{
  "name": "electrolog-app",
  "version": "1.0.0",
  "description": "Aplikasi desktop ElectroLogIXS XP4",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-packager . Electrolog --platform=win32 --arch=x64 --out=dist --icon=build/icon.ico --overwrite",
    "dist": "electron-builder --win --x64"
  },
  "build": {
    "appId": "com.electrolog.app",
    "productName": "ElectroLogIXS",
    "win": {
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^38.1.2",
    "electron-packager": "^17.1.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "crypto": "^1.0.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
